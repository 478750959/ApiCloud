/*share-option */
    .share-list{
        background-color: #F5F5F5;
        box-shadow: 0 0 5px #D6D6D6;
        z-index: 2;
    }
    .share-list li{
        height: 48px;
        display: block;
        border-bottom: 1px solid #D6D6D6;
    }
    .share-list li:active,.share-list li.tap-active{
        background-color: #7FCDE9;
    }
    .share-list .top{
        height: 64px;
    }
    .share-list .bottom{
        border-bottom: none;
    }
    .share-list .icon-box{
        width: 60px;
        height: 48px;
    }
    .share-list .top .icon-box{
        height: 64px;
    }
    .share-list .my-profile{
        position: relative;
    }
    .share-list .my-profile img{
        position: absolute;
        width: 36px;
        height: 36px;
        border-radius: 18px;
        display: block;
        left: 16px;
        top: 50%;
        -webkit-transform: translateY(-50%);
                transform: translateY(-50%);
    }
    .share-list .my-qiushi{
        background: url(../res/ic_manage_my_qiushi.png) 15px center no-repeat;
        background-size: 33px 33px;
    }
    .share-list .my-theme{
        background: url(../res/ic_theme_mode.png) 15px center no-repeat;
        background-size: 33px 33px;
    }
    .share-list .my-setting{
        background: url(../res/ic_setting.png) 15px center no-repeat;
        background-size: 33px 33px;
    }
    .share-list .my-feedback{
        background: url(../res/ic_feedback.png) 15px center no-repeat;
        background-size: 33px 33px;
    }
    .share-list li span{
        color: #030303;
        height: 48px;
        line-height: 48px; 
    }
    .share-list .top span{
        height: 64px;
        line-height: 64px; 
