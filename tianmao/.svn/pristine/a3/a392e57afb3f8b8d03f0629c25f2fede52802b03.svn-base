<script type="text/javascript" src="../../script/api.js"></script>
<script type="text/javascript" src="../../script/zepto.js"></script>
<script type="text/javascript" src="../../script/common.js"></script>
<script type="text/javascript">
    var changeTable = function(index){
        var tabs = $api.domAll('.tabs-inner');
        $api.removeCls(tabs[0],'active');
        $api.removeCls(tabs[1],'active');
        $api.removeCls(tabs[2],'active');
        $api.addCls(tabs[index],'active');
        var tabsTable = $api.domAll('.tabs-table');
        $api.removeCls(tabsTable[0],'active');
        $api.removeCls(tabsTable[1],'active');
        $api.removeCls(tabsTable[2],'active');
        $api.addCls(tabsTable[index],'active');
    };
    apiready = function(){
        Zepto(function($){
            var em = parseInt(getComputedStyle(document.body,false).fontSize);
            var emW = em*3/4;
            var bodyW = $api.offset(document.body).w;

        });
    }
</script>
