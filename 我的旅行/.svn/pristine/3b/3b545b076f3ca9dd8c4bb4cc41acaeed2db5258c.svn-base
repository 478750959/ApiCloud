* {
    -webkit-tap-highlight-color: rgba(255, 0, 0, 0);
    outline: 0;
}

html, body {
    height: 100%;
    min-width: 320px;
}

body {
    font-size: 14px;
}

.hidden {
    display: none !important;
}

.clear{clear: both;}
@font-face {font-family: 'iconfont';
    src: url('../res/iconfont/iconfont.eot'); /* IE9*/
    src: url('../res/iconfont/iconfont.eot?#iefix') format('embedded-opentype'), /* IE6-IE8 */
    url('../res/iconfont/iconfont.woff') format('woff'), /* chrome、firefox */
    url('../res/iconfont/iconfont.ttf') format('truetype'), /* chrome、firefox、opera、Safari, Android, iOS 4.2+*/
    url('../res/iconfont/iconfont.svg#iconfont') format('svg'); /* iOS 4.1- */
}
.iconfont{
	color: #FFFFFF;
    font-family:"iconfont" !important;
    font-size:2.6rem;font-style:normal;
    -webkit-font-smoothing: antialiased;
    -webkit-text-stroke-width: 0.2px;
    -moz-osx-font-smoothing: grayscale;}