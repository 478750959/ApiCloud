<script>
var reloadMe = function(){
    location.reload();
}


apiready = function(){
	
	/*// var time = new Date().getTime();
	alert(time-$api.getStorage('reload'));
	$api.setStorage('reload',time);
	// alert(typeof time);
	// alert(typeof parseInt($api.getStorage('reload')));
	// alert(parseInt($api.getStorage('reload')));
	// alert(JSON.stringify($api.getStorage('reload')));*/
	var user = $api.getStorage('user');
	if(user){
		document.getElementById('module1-item1-login').style.display='none';
		document.getElementById('module1-item1-img').style.background='url(../image/share_platform_qqfriends@2x.png) center/70% no-repeat';
		document.getElementsByClassName('module1-item1-word')[0].style.display='none';
		document.getElementById('module1-item1-account').innerHTML=user['username'];
		document.getElementById('module1-item1-account').style.display='block';
		document.getElementById('logout').style.display='block';
	}
}

document.getElementById('logout').onclick=function(){
	$api.clearStorage ();
	location.reload();
}

document.getElementById('module1-item1-login').onclick=function(){
	var user = $api.getStorage('user');
	// alert(user);
	if(user){
		alert('已登录');
		alert(JSON.stringify(user));
	} else {
	 	openWin('Win_login');
	}
}
</script>
