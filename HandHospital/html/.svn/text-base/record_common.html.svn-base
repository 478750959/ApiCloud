<!doctype html>
<html>
	<head>
		<meta charset="utf-8">
		<meta name="viewport" content="maximum-scale=1.0,minimum-scale=1.0,user-scalable=0,width=device-width,initial-scale=1.0"/>
		<title>APP</title>
		<link rel="stylesheet" type="text/css" href="../css/api.css" />
		<link rel="stylesheet" type="text/css" href="../css/common.css" />
		<link rel="stylesheet" type="text/css" href="../css/record_common.css" />
	</head>
	<body>
		<div id="wrap">
			<script id="record-template" type="text/x-dot-template">
				{{ for(var i=0; i<it.length; i++) { }}
					<section class="content" tapmode="grayActive" onclick="openDetail({{=i}})">
						<div class="left text-overflow">
							{{=it[i].num}}
						</div>
						<div class="middle text-overflow">
							{{=it[i].name}}
						</div>
						<a class="right"></a>
					</section>
				{{ } }}
			</script>
			<div id="main">
				<section class="content header_bg">
					<div class="left"></div><div class="middle">科室</div><div class="right">详情</div>
				</section>
				<section id="content">
					
				</section>
				
			</div>
		</div>
	</body>
	<script type="text/javascript" src="../script/api.js"></script>
	<script type="text/javascript" src="../script/zepto.min.js"></script>
	<script type="text/javascript" src="../script/doT.min.js"></script>
	<script type="text/javascript" src="../script/constants.js"></script>
	<script type="text/javascript" src="../script/record_common.js"></script>
</html>